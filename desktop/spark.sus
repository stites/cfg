card all {
  spark card gtk
  spark card xmonad
  spark card Xresources
}

card gtk {
  into ~
  outof gtk

  .gtkrc-2.0.mine
}

card Xresources {
  into ~
  outof Xresources

  darkpastel      -> .Xresources.darkpastel
  solarized.light -> .Xresources.solarized.light
  solarized.dark  -> .Xresources.solarized.dark
}

card x11 {
  into /etc/X11
  outof X11

  xinit/xinitrc
}

card xmonad {
  into ~/.xmonad
  outof xmonad

  xmobar.hs
  xmonad.hs
}


